    <div id="modal-invited" class="modal-invited">
      <div class="modal-form">
        <div class="modal-form-head">
          <h4 class="modal-form-title">Перевести бонусы</h4>
          <div class="close-btn-2">
            <img src="img/x-burger.svg" alt="" />
