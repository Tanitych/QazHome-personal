    <script>
      var modal2 = document.getElementById("modal-invited");
      var modalBtnRrepeat = document.getElementById("modal-btn-repeat");
      var closeBtnRepeat = document.getElementsByClassName("close-btn-2")[0];

      modalBtnRrepeat.onclick = function () {
        modal2.style.display = "block";
      };
      closeBtnRepeat.onclick = function () {
        modal2.style.display = "none";
      };
      window.onclick = function (event) {
        if (event.target == modal2) {
          modal.style.display = "none";
        }
      };
